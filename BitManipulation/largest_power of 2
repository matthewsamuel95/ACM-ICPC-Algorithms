#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int power(int n)
{
        n = n | (n>>1); 
        n = n | (n>>2);
        n = n | (n>>4);
        n = n | (n>>8);
        
        return (n+1)>>1;
}

int main()
{
    int num;
    cin>>num;
    
    int answer = power(num);
    cout<<"largest power of 2 less than"<<num<<"is"<<answer;
    return 0;
}
